{
  "name": "hand-simulator",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://masaxsuzu.github.io/hand-simulator",
  "devDependencies": {
    "@parcel/transformer-image": "^2.2.0",
    "@parcel/transformer-sass": "^2.3.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.23",
    "autoprefixer": "^9.8.8",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.3",
    "parcel": "^2.3.2",
    "postcss-modules": "^4.3.0",
    "prettier": "^2.4.1",
    "process": "^0.11.10",
    "sass": "^1.42.1",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "yarn parcel src/index.html",
    "build": "yarn parcel build src/index.html src/404.html",
    "build:gh-pages": "yarn parcel build src/index.html src/404.html  --public-url /hand-simulator --dist-dir ./dist",
    "test": "jest --env=jsdom",
    "fmt": "prettier --write src",
    "lint": "prettier --check src"
  },
  "dependencies": {
    "@fluentui/react": "^8.61.0",
    "@parcel/resolver-glob": "^2.3.2",
    "bootstrap": "5.1.3",
    "csv": "^6.0.5",
    "csv-parse": "^5.0.4",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx|js)"
    ]
  }
}
